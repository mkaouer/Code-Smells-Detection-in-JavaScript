<?xml version="1.0" encoding="UTF-8"?>
<test-data>
    <test-code>
        <description><![CDATA[
ok, calls super.clone
     ]]></description>
        <expected-problems>0</expected-problems>
        <code><![CDATA[
public class Foo {
 void clone() {
  super.clone();
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description><![CDATA[
bad, Foo.clone() calls new Foo();
     ]]></description>
        <expected-problems>1</expected-problems>
        <code><![CDATA[
public class Foo {
 void clone() {
  Foo f = new Foo();
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description><![CDATA[
clone([whatever]) is fine
     ]]></description>
        <expected-problems>0</expected-problems>
        <code><![CDATA[
public class Foo {
 void clone(String fiddle) {
  Foo f = new Foo();
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description><![CDATA[
bad, Foo.clone() calls new Foo();
     ]]></description>
        <expected-problems>1</expected-problems>
        <code><![CDATA[
public class Bar {
 class Foo {
  void clone() {
   Foo f = new Foo();
  }
 }
}
     ]]></code>
    </test-code>
</test-data>