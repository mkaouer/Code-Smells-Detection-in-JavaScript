. ./defs
set -e

# This automaton used to trigger a bug in the complementation: its
# intersection with the complement was not empty!
cat >input.tgba <<EOF
acc = "a" "b" "c";
"1", "1", "(a & d) | (a & b)", "b" "c";
"1", "2", "a & !d & !c & !b",;
"1", "1", "(d & !a) | (b & !a)", "a" "b" "c";
"1", "2", "!a & !d & !c & !b", "a";
"2", "1", "a & c & b", "b" "c";
"2", "3", "a & b & !c", "b";
"2", "2", "a & !c & !b",;
"2", "1", "c & b & !a", "a" "b" "c";
"2", "3", "b & !a & !c", "a" "b";
"2", "2", "!a & !c & !b", "a";
"3", "1", "(a & d & c) | (a & c & b)", "b" "c";
"3", "3", "(a & d & !c) | (a & b & !c)", "b";
"3", "2", "a & !d & !c & !b",;
"3", "1", "(d & c & !a) | (c & b & !a)", "a" "b" "c";
"3", "3", "(d & !a & !c) | (b & !a & !c)", "a" "b";
"3", "2", "!a & !d & !c & !b", "a";
EOF

# Check emptiness of product with complement.
run 0 ../ltl2tgba -Pinput.tgba -DC -E -C -X input.tgba
