# Check for the equals visitor

. ./defs || exit 1

check()
{
  ./equals "$2" "$3"
  test $? != $1 && exit 1;
}

# A few things which are equal
check 0 'a' 'a'
check 0 '1' '1'
check 0 '0' '0'
check 0 'a => b' 'a => b'
check 0 'G a ' ' G a'
check 0 'a U b' 'a U b'
check 0 'a & b' 'a & b'
check 0 'a & b' 'b & a'
check 0 'a & b & c' 'c & a && b'
check 0 'a & b & c' 'b & c & a'
check 0 'a && b & a' 'b & a & b'
check 0 'a & b' 'b & a & b'
check 0 'a & b' 'b & a & a'
check 0 'a & b & (c |(f U g)|| e)' 'b & a & a & (c | e |(f U g)| e | c) & b'
check 0 'a & a' 'a'

# other formulae which are not
check 1 'a' 'b'
check 1 '1' '0'
check 1 'a => b' 'b => a'
check 1 'a => b' 'a <=> b'
check 1 'a => b' 'a U b'
check 1 'a R b' 'a U b'
check 1 'a & b & c' 'c & a'
check 1 'b & c' 'c & a & b'
check 1 'a & b & (c |(f U g)| e)' 'b & a & a & (c | e |(g U g)| e | c) & b'

# Precedence
check 0 'a & b ^ c | d' 'd | c ^ b & a'

# Success.
:
