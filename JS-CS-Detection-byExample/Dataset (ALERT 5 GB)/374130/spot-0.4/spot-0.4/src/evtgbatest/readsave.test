. ./defs

set -e

cat >input <<\EOF
acc = d;
init = s1;
s1, "s2", e1, c;
"s2", "state 3", "\"he\\llo\"", c;
"state 3", "s1",e4,;
EOF

./readsave input > stdout

cat >expected <<\EOF
acc = c d;
init = s1;
s1, s2, e1, c;
s2, "state 3", "\"he\\llo\"", c;
"state 3", s1, e4,;
EOF

diff stdout expected

mv stdout input
run 0 ./readsave input > stdout

diff input stdout

rm input stdout expected
