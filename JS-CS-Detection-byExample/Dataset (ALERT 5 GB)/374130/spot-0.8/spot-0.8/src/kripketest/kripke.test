. ./defs

set -e


cat >input <<\EOF
state1, "!b", state2;
state2, "a&b", state3;
state3, "a", state4 state1;
state4, "b", state1;
EOF

run2 0 ../parse_print input

cat >input <<\EOF
state1, , state1 state2;
state2, , state1 state2;
EOF

run2 0 ../parse_print input

cat >input <<\EOF
state42, "!b & !a", state40;
state40, "!a | b", state42;
EOF

run2 0 ../parse_print input

cat >input <<\EOF
state1, "a&b", state1;
EOF

run2 0 ../parse_print input

cat >input <<\EOF
state51,,state60        state17 state3 state18 state62;
EOF

run2 0 ../parse_print input

cat >input <<\EOF
s42, "a&b|c&d", s51 s69 s73 s7;
s7, "a&a&a&!a", s42 s51 s69 s73 s42;
EOF

run2 0 ../parse_print input

