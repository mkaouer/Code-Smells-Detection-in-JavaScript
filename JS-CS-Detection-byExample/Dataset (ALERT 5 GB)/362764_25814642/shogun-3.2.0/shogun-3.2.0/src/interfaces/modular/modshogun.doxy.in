INPUT                  = @LIBSHOGUN_SRC_DIR@
OUTPUT_DIRECTORY       = modshogun
XML_OUTPUT             = doxygen_xml
CREATE_SUBDIRS         = NO
OUTPUT_LANGUAGE        = English
FILE_PATTERNS          = *.h
