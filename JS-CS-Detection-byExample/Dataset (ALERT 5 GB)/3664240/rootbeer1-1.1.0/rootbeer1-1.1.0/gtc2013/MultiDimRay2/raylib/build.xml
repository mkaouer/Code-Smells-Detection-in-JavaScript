<project>
    <target name="clean">
      <delete dir="build"/>
    </target>
    <target name="compile">
      <mkdir dir="build/classes"/>
      <javac srcdir="src" destdir="build/classes" classpath="../../../Rootbeer.jar" />
    </target>
    <target name="jar" depends="compile">
      <jar destfile="../raylib.jar" basedir="build/classes" />
    </target>
</project>
