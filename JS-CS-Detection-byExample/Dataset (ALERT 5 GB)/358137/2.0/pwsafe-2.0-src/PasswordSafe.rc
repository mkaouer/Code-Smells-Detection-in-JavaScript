IDR_MAINMENU MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New...\tCtrl+N",             ID_MENUITEM_NEW
        MENUITEM "&Open...\tCtrl+O",            ID_MENUITEM_OPEN
        MENUITEM SEPARATOR
        MENUITEM "Recent",                      ID_FILE_MRU_ENTRY1
        MENUITEM SEPARATOR
        MENUITEM "&Save\tCtrl+S",               ID_MENUITEM_SAVE, GRAYED
        MENUITEM "Save &As...",                 ID_MENUTIME_SAVEAS
        MENUITEM SEPARATOR
        POPUP "Export &To"
        BEGIN
            MENUITEM "O&ld (1.x) format...",        ID_FILE_EXPORTTO_OLD1XFORMAT

            MENUITEM "&Plain Text...",              ID_FILE_EXPORTTO_PLAINTEXT

        END
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tCtrl+Q",               ID_MENUITEM_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Add Entry..\tCtrl+A",        ID_MENUITEM_ADD
        MENUITEM "Edit/&View Entry...\tCtrl+Enter", ID_MENUITEM_EDIT
        MENUITEM "&Delete Entry\tDel",          ID_MENUITEM_DELETE
        MENUITEM "&Find Entry...\tCtrl+F",      ID_MENUITEM_FIND
        MENUITEM SEPARATOR
        MENUITEM "&Copy Password to Clipboard\tCtrl+C", 
                                                ID_MENUITEM_COPYPASSWORD
        MENUITEM "Copy &Username to Clipboard\tCtrl+U", 
                                                ID_MENUITEM_COPYUSERNAME
        MENUITEM "C&lear Clipboard\tCtrl+Del",  ID_MENUITEM_CLEARCLIPBOARD
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&List",                       ID_MENUITEM_LIST_VIEW
        , CHECKED
        MENUITEM "&Tree",                       ID_MENUITEM_TREE_VIEW
        , CHECKED
    END
    POPUP "&Manage"
    BEGIN
        MENUITEM "&Change Safe Combination...", ID_MENUITEM_CHANGECOMBO
        MENUITEM SEPARATOR
        MENUITEM "Make &Backup\tCtrl+B",        ID_MENUITEM_BACKUPSAFE
        MENUITEM "&Restore from Backup...\tCtrl+R", ID_MENUITEM_RESTORE
        MENUITEM SEPARATOR
        MENUITEM "&Options...",                 ID_MENUITEM_OPTIONS
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "Get &Help\tF1",               ID_HELP
        MENUITEM SEPARATOR
        MENUITEM "&About Password Safe...",     ID_MENUITEM_ABOUT
    END
END

